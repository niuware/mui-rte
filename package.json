{
  "name": "@swipejobs/mui-rte",
  "version": "1.0.0",
  "description": "Forked mui-rte v2.0.1 (Material-UI Rich Text Editor and Viewer)",
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "ts-patch install -s",
    "start": "start-storybook -p 6006",
    "build": "npm run build:index && rollup --config --bundleConfigAsCjs",
    "build:index": "node exportsBuild.mjs && npm run prettier",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:local": "npm run build && npm pack",
    "clean": "rm -rf dist && rm *.tgz",
    "prettier": "prettier --write --ignore-unknown .",
    "release": "semantic-release",
    "storybook": "npm run start",
    "build-storybook": "build-storybook",
    "test": "npm run build && jest --clearCache && jest --coverage --maxWorkers=50% --silent",
    "first-time-setup": "npm i && npm-install-peers"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.24",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/draft-js": "^0.11.5",
    "@types/jest": "^29.2.4",
    "@types/lodash-es": "^4.17.3",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^29.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "colors": "1.4.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rollup": "^3.29.4",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^19.0.5",
    "semantic-release-slack-bot": "^3.5.3",
    "ts-jest": "^29.0.3",
    "ts-patch": "^2.0.1",
    "tslib": "^2.6.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.0.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "core-js": "^3.8.2",
    "draft-js": "^0.11.7",
    "immutable": "^3.8.2",
    "lodash-es": "^4.17.15",
    "regenerator-runtime": "^0.13.7"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "@mui/icons-material": "^5.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "clsx": "^1.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
